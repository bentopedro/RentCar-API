    "paths":{
        "/categories": {
            "post":{
                "tags":["Category"],
                "summary": "Create a category",
                "description": "Create a new category",
                "requestBody": {
                    "content":{
                        "application/json":{
                            "schema":{
                                "type":"object",
                                "properties":{
                                    "name": {
                                        "type":"string"
                                    },
                                    "description": {
                                        "type":"string"
                                    }
                                },
                                "examples":{
                                    "name":"Category Name",
                                    "description":"Category Description"
                                }
                            }
                        }
                    }
                },
                "responses": {
                    "201":{
                        "description": "Created category"
                    },
                    "500":{
                        "description": "Category could not be created"
                    }
                }
            }
        }
    }
}